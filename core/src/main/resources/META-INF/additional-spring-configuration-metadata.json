{
  "groups": [
    {
      "name": "spring.sequence",
      "description": "自定义序列.",
      "type": "net.asany.jfantasy.autoconfigure.properties.SequenceProperties",
      "sourceType": "net.asany.jfantasy.autoconfigure.properties.SequenceProperties"
    },
    {
      "name": "spring.cache.advance",
      "description": "缓存提前初始化.",
      "type": "net.asany.jfantasy.autoconfigure.properties.CacheAdvanceProperties",
      "sourceType": "net.asany.jfantasy.autoconfigure.properties.CacheAdvanceProperties"
    },
    {
      "name": "spring.security",
      "description": "安全配置.",
      "type": "net.asany.jfantasy.autoconfigure.properties.SecurityProperties",
      "sourceType": "net.asany.jfantasy.autoconfigure.properties.SecurityProperties"
    }
  ],
  "properties": [
    {
      "name": "spring.tomcat.disableSession",
      "type": "java.lang.String",
      "description": "禁用 Tomcat 的 Session."
    },
    {
      "name": "spring.sequence.pool-size",
      "type": "java.lang.Integer",
      "description": "数据库自定义序列池大小."
    },
    {
      "name": "spring.datasource.proxy",
      "type": "java.lang.Boolean",
      "description": "启用数据源代理.",
      "sourceType": "net.asany.jfantasy.autoconfigure.properties.DataSourceExtendedProperties"
    },
    {
      "name": "spring.cache.advance.bean-names",
      "type": "java.lang.String",
      "description": "Bean Names."
    },
    {
      "name": "spring.security.token-services",
      "type": "java.util.Map<java.lang.String, java.lang.String>",
      "description": "Token Services."
    },
    {
      "name": "spring.security.access-token.validate",
      "type": "java.lang.Boolean",
      "description": "是否进行token验证.",
      "defaultValue": "true"
    },
    {
      "name": "spring.security.access-token.refresh",
      "type": "java.lang.Boolean",
      "description": "是否刷新token.",
      "defaultValue": "true"
    },
    {
      "name": "spring.security.access-token.validation-endpoint",
      "type": "java.lang.String",
      "description": "验证token的URL."
    }
  ],
  "hints": [
    {
      "name": "spring.security.token-services.keys",
      "values": [
        {
          "value": "api-key",
          "description": "Description of what 'apiKey' configures."
        },
        {
          "value": "access-token",
          "description": "Description of what 'accessToken' configures."
        }
      ]
    }
  ]
}